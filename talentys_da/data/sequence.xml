<odoo>
    <data>
        <record id="sequence_purchase_purchase_request" model="ir.sequence">
            <field name="name">Demande d'achat sequence</field>
            <field name="code">purchase.purchase.request</field>
            <field name="prefix">DA/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>
    </data>
</odoo>